{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.private-types/**": true,
    "**/.test-js/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    ".test-js/**": true,
    "dist/**": true,
    ".private-types/**": true
  },
  "files.associations": {
    "api-extractor.json": "jsonc"
  },
  "eslint.enable": true,
  "typescript.tsdk": "node_modules/typescript/lib"
}
